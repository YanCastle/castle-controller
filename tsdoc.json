{
	"tags": {
		"allowUnknownTags": true
	},
	"plugins": [
		"plugins/markdown",
		"/Users/castle/.config/yarn/global/node_modules/tsdoc/template/plugins/TSDoc.js"
	],
	"opts": {
		"template": "/Users/castle/.config/yarn/global/node_modules/tsdoc/template",
		"recurse": "true"
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"source": {
		"includePattern": "(\\.d)?\\.ts$"
	},
	"markdown": {
		"parser": "gfm",
		"hardwrap": true
	},
	"tsdoc": {
		"source": "src/",
		"destination": "docs",
		"tutorials": "",
		"systemName": "castle-controller",
		"footer": "",
		"copyright": "castle-controller Copyright Â© 2022 castle.",
		"outputSourceFiles": true,
		"commentsOnly": true
	}
}